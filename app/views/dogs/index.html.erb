<h1>Listing dogs</h1>

<table>
  <tr>
    <th>Dname</th>
    <th>Breed</th>
    <th>Allergies</th>
    <th>Weight</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dogs.each do |dog| %>
  <tr>
    <td><%= dog.dname %></td>
    <td><%= dog.breed %></td>
    <td><%= dog.allergies %></td>
    <td><%= dog.weight %></td>
    <td><%= link_to 'Show', dog %></td>
    <td><%= link_to 'Edit', edit_dog_path(dog) %></td>
    <td><%= link_to 'Destroy', dog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dog', new_dog_path %>
